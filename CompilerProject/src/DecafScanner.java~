          case 29:
            { OutputHandler.handle(Token.T_OR);
            }
            // fall through
          case 89: break;
          case 30:
            { OutputHandler.handle(Token.T_for);
            }
            // fall through
          case 90: break;
          case 31:
            { OutputHandler.handle(Token.T_int);
            }
            // fall through
          case 91: break;
          case 32:
            { OutputHandler.handle(Token.T_new);
            }
            // fall through
